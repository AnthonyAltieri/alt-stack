    "express": "^4.0.0 || ^5.0.0",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/supertest": "^6.0.3",
    "express": "^5.0.0",
    "oxlint": "^1.24.0",
    "supertest": "^7.1.4",
