{
  "name": "@alt-stack/zod-asyncapi",
  "version": "1.1.0",
  "description": "Convert AsyncAPI schemas to Zod schemas with TypeScript code generation",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "bin": {
    "zod-asyncapi": "./bin/zod-asyncapi.js"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "check-types": "tsc --noEmit"
  },
  "keywords": [
    "asyncapi",
    "kafka",
    "zod",
    "typescript",
    "code-generation"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "oxlint": "^1.24.0",
    "tsup": "^8.0.0",
    "typescript": "^5.9.2",
    "vitest": "^4.0.3"
  },
  "dependencies": {
    "tsx": "^4.19.2",
    "zod": "^4.1.12"
  },
  "_publishTrigger": 1765256199110
}
