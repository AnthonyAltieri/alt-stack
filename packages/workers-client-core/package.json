{
  "name": "@alt-stack/workers-client-core",
  "version": "0.2.2",
  "description": "Core types and interfaces for type-safe worker clients",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "workers",
    "client",
    "typescript",
    "zod",
    "trigger.dev",
    "warpstream"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "zod": "^3.25.0 || ^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.9.2",
    "vitest": "^4.0.3",
    "zod": "^4.0.0"
  }
}
