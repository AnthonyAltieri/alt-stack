  "dependencies": {
    "@alt-stack/server-core": "workspace:*"
  },
  "peerDependencies": {
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "bun-types": "^1.2.5",
    "oxlint": "^1.24.0",
    "tsup": "^8.0.0",
    "typescript": "^5.9.2",
    "vitest": "^4.0.3",
    "zod": "^4.0.0"
  }
}
