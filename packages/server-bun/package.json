{
  "name": "@alt-stack/server-bun",
  "version": "0.4.3",
  "description": "Type-safe server framework built on Bun with Zod validation",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
